{
  "name": "bingo",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://web2098.github.io/BingoBoard",
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.101",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "babel": "^6.23.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.24.1",
    "react-scripts": "5.0.1",
    "swiper": "^11.1.4",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "debug": "react-scripts start --inspect",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(swiper|ssr-window|dom7)/)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/reportWebVitals.ts"
    ],
    "moduleNameMapper": {
      "^swiper/react$": "<rootDir>/src/__mocks__/swiper/react.js",
      "^swiper/modules$": "<rootDir>/src/__mocks__/swiper/modules.js",
      "^swiper/css$": "identity-obj-proxy",
      "^swiper/css/navigation$": "identity-obj-proxy",
      "^swiper/css/pagination$": "identity-obj-proxy",
      "^swiper/css/mousewheel$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "gh-pages": "^6.1.1",
    "identity-obj-proxy": "^3.0.0"
  }
}
